<div class="row">
  <div class="store-grid">
    <div class="row">
      <%= render "stores/store_head", store: entertainment_store %>
    </div>
    <div class="row"> 
      <%= render "stores/store_main", store: entertainment_store %>
    </div>
    <div class="row">
      <% if entertainment_store.tags.any? then %>          
        <% entertainment_store.tags.each do |tag| %>
          <%= tag.title %>
        <% end %> 
      <% end %>
    </div>
  </div>
</div>



